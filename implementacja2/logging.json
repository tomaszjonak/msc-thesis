{
  "version": 1,
  "formatters": {
    "standard": {"format": "%(asctime)s [%(name)s|%(threadName)s] %(levelname)s: %(message)s"}
  },
  "handlers": {
    "console": {
      "formatter": "standard",
      "level": "DEBUG",
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stdout"
    }
  },
  "root": {
    "level": "DEBUG",
    "handlers": ["console"]
  }
}
